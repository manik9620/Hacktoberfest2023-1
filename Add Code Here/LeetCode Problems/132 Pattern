class Solution {
public:
    bool find132pattern(vector<int>& nums) {
        stack<int>s;
        int m=INT_MIN;
        int n=nums.size();
        for(int i=n-1;i>=0;i--){
            if(nums[i]<m){
                return true;
            }
            else{
                while(!s.empty() && nums[i]>s.top()){
                    m=s.top();
                    s.pop();
                }
                s.push(nums[i]);
            }
            
        }
        return false;
    }
};
